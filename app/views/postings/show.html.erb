<h6>These are your postings</h6>

<% if @postings %>
    <table id="user_postings">
         <thead>
            <tr>
                <th>Post Number</th>
                <th>Description  </th>
            
            </tr>
        </thead>   
        <tbody>
            <% @postings.each do |post| %>
                <tr>
                    <td><%= post.id %></td>
                    <td><%= post.name %></td>
            
                </tr>
            <% end %>

        </tbody>
    </table>
<% else %>
    <p>You don't have any postings at this time.</p>
<% end %>
